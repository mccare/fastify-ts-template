{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "baseBranches": ["master"],
  "packageRules": [
    {
      "automerge": true,
      "matchUpdateTypes": ["*"],
      "groupName": "all dependencies",
      "labels": ["dependencies"],
      "matchPackageNames": ["*"]
    }
  ]
}
